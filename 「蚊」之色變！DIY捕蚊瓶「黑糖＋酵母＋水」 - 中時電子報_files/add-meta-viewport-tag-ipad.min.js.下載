"use strict";var addMetaViewPortTag=function(n){if(n.indexOf(baseUrl)>-1)return!1;(/ipad/gi.test(navigator.platform)||/ipad.+Safari/gi.test(navigator.userAgent))&&($("head").find('meta[name="viewport"]').remove(),$("head").append('<meta name="viewport" content="width=device-width">'))};addMetaViewPortTag(window.location.href.toLowerCase())