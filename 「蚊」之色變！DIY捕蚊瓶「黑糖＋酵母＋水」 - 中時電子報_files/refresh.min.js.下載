function wwwRefresh(){var a=300000;this.SetDelaySec=function(c){a=c*1000*60};this.Start=function(){b()};function b(){var d=$(".video_view").length;if(d==0&a>0){var e;function c(){clearTimeout(e);if(a===0){return}e=setTimeout(function(){$.ajax({type:"GET",url:"http://reload.chinatimes.com/api/SleepCounter/"});window.location.reload()},a)}c();$("body").click(function(){c()});$(window).scroll(function(){c()});$(window).resize(function(){c()});$(document).keypress(function(f){c()})}}};